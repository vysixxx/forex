import matplotlib.pyplot as plt
import pandas as pd
import numpy as np

series = pd.read_csv('C:/Users/u≈æivatel/AppData/Roaming/MetaQuotes/Terminal/D1947BEB86901215B88312DFDDF3CB1C/MQL4/Files/EURUSD.txt', header=1, parse_dates=None, index_col=0, squeeze=True)

print(type(series))
print(series.head(10))
seriesClose=series['CLOSE']
seriesOpen=series['OPEN']
Close=np.array(200)
Open=np.array(200)
Close=np.copy(seriesClose)
Open=np.copy(seriesOpen)
RW=Close-Open

for index in range(len(RW)):
    RW[index]=RW[index]+RW[index-1]

plt.plot(RW)
